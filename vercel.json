{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "outputDirectory": ".",
  "installCommand": "npm install",
  "framework": "nodejs",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "PGHOST": "@pghost",
    "PGDATABASE": "@pgdatabase",
    "PGUSER": "@pguser",
    "PGPASSWORD": "@pgpassword"
  }
}